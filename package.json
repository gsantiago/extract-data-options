{
  "name": "extract-data-options",
  "version": "0.0.1",
  "description": "Extract `data-(namespace)-*` options from a HTML element",
  "keywords": [
    "data",
    "options",
    "object",
    "json",
    "settings",
    "custom"
  ],
  "scripts": {
